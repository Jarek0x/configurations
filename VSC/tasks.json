{
    "version": "2.0.0",
    "tasks": [
        {
            "taskName": "CloudFormation AWS Docs Search",
            "isBuildCommand": true,
            "command": "Firefox",
            "windows": {
                "command": "C:/Program Files (x86)/Mozilla Firefox/firefox.exe"
            },
            "args": [
                "https://docs.aws.amazon.com/search/doc-search.html?searchPath=documentation&searchQuery=${selectedText}&x=29&y=11#facet_doc_guide=User+Guide&facet_doc_product=AWS+CloudFormation"
            ]
        }
    ]
}
